{
    "name": "truefrontier/color",
    "description": "A fork of spatie/colors: A little library to handle color conversions",
    "keywords": [
        "spatie",
        "color",
        "conversion",
        "rgb"
    ],
    "homepage": "https://github.com/spatie/color",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Kevin Kirchner",
            "email": "kevnk@hey.com",
            "homepage": "https://truefrontierapps.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5||^9.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Color\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Color\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
